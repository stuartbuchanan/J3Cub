   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>3000.0</ind>
    <dep>229</dep>
   </entry>
  </interpolation>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
    <x-m>-0.17094</x-m>
    <y-m> 0.06501</y-m>
    <z-m>-0.37776</z-m>
  </center>
 </animation>

</PropertyList>


