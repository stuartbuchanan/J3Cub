            </property-rule>
            
            <property-rule n="108">
                <path>Systems/engine.xml</path>
            </property-rule>
        </systems>

        <help include="J3Cub-help.xml"/>

        <menubar include="Dialogs/J3Cub-menu.xml"/>

        <multiplay>
            <chat_display>1</chat_display>
            <generic>
                <float n="0" alias="/sim/model/door-positions/rightDoor/position-norm"/>
